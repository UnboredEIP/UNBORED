{
  "expo": {
    "name": "UnBored",
    "scheme": "unboredapp",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "custom photos permission",
          "cameraPermission": "Allow $(PRODUCT_NAME) to open the camera",
          "//": "Disables the microphone permission",
          "microphonePermission": false
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          },
          "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.unboredeip.unboredapp",
            "buildNumber": "3"
          }
        }
      ],
      [
        "expo-notifications"
      ],
      "@react-native-google-signin/google-signin",
      "expo-font"
    ],
    "slug": "unbored2",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/unBored.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/unBored.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.unboredeip.unboredapp"
    },
    "android": {
      "useNextNetworkingApis": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.unboredeip.unboredapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "df2f28a1-374e-4e2c-852e-855f117615d9"
      }
    }
  }
}
